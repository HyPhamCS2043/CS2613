#lang racket
(define (larger-rectangle l1 w1 l2 w2)
  (define (rect-area length width)
    (* length width))
  (cond
    [(> (rect-area l1 w1)(rect-area l2 w2)) -1]
    [(< (rect-area l1 w1)(rect-area l2 w2)) 1]
    [else 0]))


(displayln (larger-rectangle 1 1 2 2))
(displayln (larger-rectangle 2 27 6 7))
(displayln (larger-rectangle 2 21 6 7))
(displayln (larger-rectangle 15 10 11 12))
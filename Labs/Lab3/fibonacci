#lang racket
(define (fib n)
  (cond
    [(equal? n 0) 0]
    [(equal? n 1) 1]
    [(> n 1) (+(fib (- n 1))(fib(- n 2)))]))

(define (testFib n)
  (display "The fibonacci number at the ")
  (display n)
  (display "th position is ")
  (displayln (fib n)))

(testFib 2)
(testFib 3)
(testFib 13)
(testFib 15)